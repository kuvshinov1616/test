#!groovy
// Check gittest properties
properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'master'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        
        }
    stages {
        stage("First step") {
            steps {
                sh 'ssh root@gittest.ncsd.ru \'hostname\''
            }
        }
        stage("Second step") {
            steps {
                sh 'ssh root@gittest.ncsd.ru \'uptime\''
            }
        }
    }
}
